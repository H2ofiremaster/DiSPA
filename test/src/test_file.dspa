object dtest;
# @ is delay, % is duration.

@0 %20 move e1 0 1 0;
@10 {
    %21 move e1 ~ ~1 ~;
    %22 turn e1 90 0 0;
}
@20 {
    %23 move e1 1 0 0; # This is a comment, by the way.
    %24 turn e1 0 90 0;

}

@30 %20 {
    move e1 ~1 ~ ~;
    %10 {
        move e1 ~ ~ ~1;
        turn e1 ~90 ~ ~;
        %100 size e1 2 2 2;
    }
    size e1 2 2 2;
}
@100 end;
